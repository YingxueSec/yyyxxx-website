<script is:inline>
  // 计算运行时间（从 2025-11-20 开始）
  function updateRuntime() {
    const startDate = new Date('2025-11-20T00:00:00');
    const now = new Date();
    const diff = now.getTime() - startDate.getTime();
    const days = Math.floor(diff / (1000 * 60 * 60 * 24));
    const runtimeElement = document.getElementById('runtime_days');
    if (runtimeElement) {
      runtimeElement.textContent = String(days);
    }
  }
  updateRuntime();
  setInterval(updateRuntime, 1000 * 60 * 60 * 24);
</script>

